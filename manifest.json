{
	"manifest_version": 2,
	"name": "Foobar",
	"description": "Making developers life easy.",
	"version": "2.1.0",
	"short_name": "Foobar",
	"browser_action": {
		"default_icon": "images/logo.png",
		"default_popup": "index.html",
		"default_title": "Foobar"
	},
	"background": {},
	"icons": {
		"128": "images/logo.png"
	},
	"permissions": [
		"storage",
		"tabs"
	]
}